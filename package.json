{
  "name": "seeme-client",
  "version": "0.1.0",
  "description": "Nodejs client library for Seeme.hu sms gateway",
  "keywords": [
    "seeme",
    "send",
    "sms",
    "gateway",
    "api",
    "client"
  ],
  "main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --ci --coverage",
    "test:coverage": "jest --coverage",
    "test:dev": "jest --watch",
    "prerelease": "npm run test && npm run build",
    "release": "standard-version -a",
    "release:patch": "npm run release -- -r patch",
    "release:minor": "npm run release -- -r minor",
    "release:major": "npm run release -- -r major"
  },
  "author": "Dániel Sebestyén <dannystyleart@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dannystyleart/seeme-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "github:dannystyleart/seeme-client"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.7",
    "jest": "^26.6.3",
    "msw": "^0.25.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "standard-version": "^9.1.0"
  },
  "peerDependencies": {
    "node-fetch": "^2.6.1"
  }
}
